package pattern_printing;

public class AlphabetsSquare {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 5;
         for(int i=1;i<=n;i++) {
        	 int a=1;
        	 for(int j=1;j<=n;j++) {
        		// int d = a +64;
        		// char ch= (char)d;
        		 System.out.print(a+64);
        		  a=a++;
        	 }
        	 System.out.println();
         }
	}

}
