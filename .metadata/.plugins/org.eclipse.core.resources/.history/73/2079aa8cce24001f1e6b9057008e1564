package pattern_printing;

public class FloydTriangle123 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n = 5;
		int a = 1;
		for(int i = 1; i<=n;i++) {
			 
			for ( int j=1;j<=i;j++) {
				System.out.print(a);

				a++;
			}
//			a++;
			System.out.println(); 
		}

	}

}
