package pattern_printing;

public class kPattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// TODO Auto-generated method stub
 for (int i =1; i<=5;i++) {
	 for(int j=1; j<=6-i;j++) {
		 System.out.print("*");
	 }
	 System.out.println();

	}
	// TODO Auto-generated method stub
for (int i =1; i<=5;i++) {
for(int j=1; j<=i;j++) {
	 System.out.print("*");
}
System.out.println();
	}
	
	}
}
