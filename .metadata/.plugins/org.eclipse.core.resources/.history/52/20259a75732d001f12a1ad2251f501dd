package june18.array;

public class Demo2 {

	public static void main(String[] args) {
		
		 int [] arr = { 23,27,64,75,83,24} ;
		 boolean flag = false;
		 for(int i:arr)
		 {
		 if(i == 83) {
			 flag = true;
			 System.out.println("yes");
			
		 }
	}
		 
		 if(flag == false) {
			 System.out.println("No");
		 }
}
}
