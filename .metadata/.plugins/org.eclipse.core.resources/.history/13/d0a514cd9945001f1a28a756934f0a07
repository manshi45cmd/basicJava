package array;

public class MiscProblems {
	
	static void swap(int a, int b) {
		System.out.println("original value before swap :");
		System.out.println("a :"+ a);
		System.out.println("b :" + b);
		
		int temp = a;
		a = b;
		b = temp;
	}
	
	public static void main(String[] args) {
		
		int a = 9;
		int b = 3;
		
		 swap(a,b);
	}

}
