package string;

public class Example1 {

	public static void main(String[] args) {
		 
		String s = "Manshi ";
		String ss = "Neha";
		s= s+ss;
		System.out.println(s);
		s= s+" Shrivastav";
		System.out.println(s);
		
		s= s.toUpperCase();
		System.out.println("UperCase " +s);
		
		System.out.println("Caontains "+ s.contains(ss));
		
		
		

	}

}
