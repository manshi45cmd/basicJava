package june19.multidimenstional;

public class MDA {
	
	public static void main(String[] args) {
		int [] [] mat = {{2,4,6},{8,10,12},{14,16,18}};
		for(int i = 0; i<=3;i++)
		{
			for(int j = 0; j<=3;j++) {
				if(i%2==0) {
					System.out.print(mat[i][j]);
				}
			}
//			
			System.out.println("");
	}
	}
	
	}
}
