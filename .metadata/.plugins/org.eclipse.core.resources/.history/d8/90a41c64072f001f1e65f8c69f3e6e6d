package june19.multidimenstional;

public class CountSize {
  
	public static void main(String[] args) {
		int [] arr = {0,1,0,2,1,2,0,1,2,0};
		int n = 10;
		for(int i = 1; i<10;i++) {
			if(i==0) {
				System.out.print(arr[i]);
			}else if (i==1) {
					System.out.print(i);
				} else if(i==2) {
					System.out.print(i);
					
				}
			}
		}
	}

