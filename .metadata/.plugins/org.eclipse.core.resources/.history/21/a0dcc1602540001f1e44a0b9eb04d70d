package string;
import java.util.*;
public class StringBuilderSB {
	
	public static void main(String[] args) {
		
		StringBuilder str = new StringBuilder("Hello");
	System.out.println(str);
	
	 str.setCharAt(0,'m');
	 System.out.println(str);
	 
//	 str.append(false);
//	 System.out.println(str);
	 
	 str.append('o');
	 System.out.println(str);
	 
	 str.insert(3, 'y');
	 System.out.println(str);
	}

}
