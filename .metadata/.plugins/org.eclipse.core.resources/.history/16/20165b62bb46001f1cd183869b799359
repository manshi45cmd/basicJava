package array;

public class Snippet {
	
	 Scanner sc = new Scanner(System.in);
	 System.out.println("enter array size:");
	 int n = sc.nextInt();
	
	 int[]arr = new int[n];
	
	 System.out.println("Enter " + n + " elements");
	 for(int i =0; i<arr.length;i++) {
		  arr[i]  = sc.nextInt();
		 
	 }
	 System.out.println("Enter K : ");
	 int k = sc.nextInt();
	
}

