package string;
import java.util.*;
public class StringBuilderSB {
	
	public static void main(String[] args) {
		
		StringBuilder str = new StringBuilder("Hello");
	System.out.println(str);
	
	 str.setCharAt(0,'m');
	 System.out.println(str);
	 
	 str.append(false);
	 System.out.println(str);
	 
	 str.append(4);
	 System.out.println(str);
	}

}
